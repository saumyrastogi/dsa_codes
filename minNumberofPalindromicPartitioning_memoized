#include<bits/stdc++.h> 
#include<string>
#include <iostream>
using namespace std;

int t[1001][1001];
string lcs(string &X, string &Y, int m, int n) 
{ 
    int i,j;
    for(i=1;i<=m;i++)
    {
        for(j=1;j<=n;j++)
        {
            if(X[i-1]==Y[j-1])
            t[i][j]=1+t[i-1][j-1];
            else
            t[i][j]=max(t[i-1][j] , t[i][j-1]);
        }
    }
    string str;
    i=m,j=n;
    while(i>0 && j>0)
    {
        if(X[i-1]==Y[j-1])
        {
            str.push_back(X[i-1]);
            i--;
            j--;
        }
        else
        {
            if(t[i][j-1]>t[i-1][j])
            {
                str.push_back(Y[j-1]);
                j--;
            }
                
            else
            {
                str.push_back(X[i-1]);
                i--;
            }
        }
    }
    while(i>0)
    {
        str.push_back(X[i-1]);
        i--;
    }
    while(j>0)
    {
        str.push_back(Y[j-1]);
        j--;
    }
    return str;
} 

int max(int a, int b) 
{
    return (a > b)? a : b; 
}

int main() 
{ 
string X,Y;
cin>>X>>Y;
int m = X.size(); 
int n = Y.size(); 
memset(t,0,sizeof(t));
string str=lcs( X, Y, m, n );
reverse(str.begin(),str.end());
cout<<str;

return 0; 
} 
