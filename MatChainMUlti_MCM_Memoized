#include<bits/stdc++.h> 
#include<string>
#include <iostream>
using namespace std;

int t[1001][1001];
int ans = INT_MAX;
int mcm(int arr[] , int i,int j)
{
    int k;
    
    if(i>=j)
    return 0;
    
    if(t[i][j]!=-1)
    return t[i][j];
    
    int min=INT_MAX;
    for(k=i;k<j;k++)
    {
        t[i][j] = mcm(arr,i,k) + mcm(arr,k+1,j) + arr[i-1]*arr[k]*arr[j];
        if(t[i][j]<min)
        min=t[i][j];
    }
    return min;
}

int main() 
{ 
   int n,i;
   cin>>n;
   int arr[n];
   for(i=0;i<n;i++)
   cin>>arr[i];
   memset(t,-1,sizeof(t)) ;
   ans = mcm(arr , 1 , n-1);
   cout<<ans;

return 0; 
} 
