#include <iostream>
#include <bits/stdc++.h> 
using namespace std;
int static t[100][100];
int subsetsum(int arr[6],int n,int sum,int start)
{   
    int i;
    if(start>=n && sum>0)
    {
        return 0;
    }
    if(sum==0)
    {
        return 1;
    }
    if(t[sum][start]!=-1)
    return t[sum][start];
    
    
    for(i=start;i<n;i++)
    {
        if(arr[i]<=sum)
        {
            
            t[sum-arr[i]][i+1]=subsetsum(arr,n,sum-arr[i],i+1);
            
            if(t[sum-arr[i]][i+1]==1)
            return 1;
            
        }
    }
    return 0;
}
int main() {
    memset(t,-1,sizeof(t));
	int n=6,sum=21,start=0;
	int arr[6]={4,10,12,17,15,5};

	if(subsetsum(arr,n,sum,0)==1)
	cout<<"true";
	else
	cout<<"false";
	
	return 0;
}
