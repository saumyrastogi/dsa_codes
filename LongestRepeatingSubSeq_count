#include<bits/stdc++.h> 
#include<string>
#include <iostream>
using namespace std;

int t[1001][1001];
int lcs(string &X, string &Y, int m, int n ) 
{ 
    int i,j;
    for(i=1;i<=m;i++)
    {
        for(j=1;j<=n;j++)
        {
            if(X[i-1]==Y[j-1] && i!=j)                                                                       // only change in this line rest same as lcs
            t[i][j]=1+t[i-1][j-1];
            else
            t[i][j]=max(t[i-1][j] , t[i][j-1]);
        }
    }
    return t[m][n];
} 

int max(int a, int b) 
{
    return (a > b)? a : b; 
}

int main() 
{ 
string X,Y;
cin>>X>>Y;
int m = X.size(); 
int n = Y.size(); 
memset(t,0,sizeof(t));
printf("Length of LRS is %d", lcs( X, Y, m, n ) ); 

return 0; 
} 
